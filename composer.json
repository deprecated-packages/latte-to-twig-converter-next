{
    "name": "symplify/latte-to-twig-converter",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": [
        "bin/latte-to-twig"
    ],
    "require": {
        "php": ">=8.0",
        "nette/utils": "^3.2",
        "symfony/dependency-injection": "^5.3",
        "symfony/console": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/finder": "^5.3",
        "symplify/symplify-kernel": "^9.4.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
