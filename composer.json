{
    "name": "symplify/latte-to-twig-converter",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": [
        "bin/latte-to-twig"
    ],
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "symfony/dependency-injection": "^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/finder": "^4.4|^5.1",
        "symplify/symplify-kernel": "^9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\LatteToTwigConverter\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
